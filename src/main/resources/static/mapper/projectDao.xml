    <insert id="addAlterationDao" parameterType="com.goodsoft.yuanlin.domain.entity.project.Alteration">
    INSERT INTO hz7.gs_change_management
    (PID,ID,UIDS,DEPTID,CONFIRMATION_PARTY,CHANGE_DATE,CHANGE_BASIS)
    VALUES (#{pid},#{id},#{uid},#{deptId},#{confParty},#{time},#{filesId})
