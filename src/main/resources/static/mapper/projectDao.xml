    <resultMap id="alterationInfo" type="com.goodsoft.yuanlin.domain.entity.project.Alteration">
        <result property="name" column="PROJECT_NAME"/>
        <result property="id" column="ID"/>
        <result property="confParty" column="CONFIRMATION_PARTY"/>
        <result property="time" column="CHANGE_DATE"/>
        <result property="filesId" column="CHANGE_BASIS"/>
    </resultMap>
    <!--变更管理sql-->
    <select id="queryAlterationDao" parameterType="java.lang.String" resultMap="alterationInfo">
        SELECT
        A.ID AS PID,
        A.PROJECT_NAME,
        B.ID,
        B.CONFIRMATION_PARTY,
        B.CHANGE_DATE,
        B.CHANGE_BASIS,
        B.UIDS,
        B.DEPTID
        FROM
        hz7.gs_project_information A
        LEFT OUTER JOIN
        hz7.gs_change_management B ON A.ID = B.PID
        <where>
            <if test="deptId !=null and deptId !=''">
                and B.DEPTID=#{deptId}
            </if>
            <if test="uid !=null and uid !=''">
                and B.UIDS=#{uid}
            </if>
        </where>
        order by B.CHANGE_DATE limit #{page},20
    </select>
